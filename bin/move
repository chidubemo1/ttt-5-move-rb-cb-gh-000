#!/usr/bin/env ruby

require_relative '../lib/move.rb'
puts "Welcome to Tic Tac Toe!"
board = [" ", " ", " ", " ", " ", " ", " ", " ", " "]
puts "Where would you like to go?"

9.times do
  uinput = gets.strip
  index = input_to_index(uinput)
  move(board, index)
  display_board(board)
end

